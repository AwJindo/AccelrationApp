<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.accelrationapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AccelrationApp">
        <activity android:name=".ExistingUserActivity" android:parentActivityName=".MainActivity"></activity>

        <activity android:name=".UserInfoActivity" android:parentActivityName=".NewUserActivity"></activity>

        <activity android:name=".ShowValuesFromHistoryActivity" android:parentActivityName=".HistoryActivity"></activity>

        <activity android:name=".AcceptTestValuesActivity" ></activity>
        <activity android:name=".HistoryActivity" android:parentActivityName=".UserInfoActivity"></activity>

        <activity android:name=".StopActivity" ></activity>

        <activity android:name=".StartActivity" android:parentActivityName=".UserInfoActivity"></activity>

        <activity android:name=".NewUserActivity" android:parentActivityName=".MainActivity"></activity>

        <activity android:name=".MainActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
    </application>

</manifest>